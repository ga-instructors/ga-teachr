<div class="large container">
  <h1>Survey <%= @survey_questionnaire.cohort.name %></h1>

  <%= form_for(@survey_questionnaire) do |form| %>
    <%= render form %>
    <div class="actions">
      <%= form.submit 'Continue' %>
      <%= link_to 'Cancel', survey_questionnaires_path %>
    </div>
  <% end %>

</div>
